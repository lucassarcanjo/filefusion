{
  "bindings": [
    {
      "authLevel": "Anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "storage",
      "type": "blob",
      "path": "app-files/{filename}",
      "direction": "out",
      "connection": "AzureWebJobsStorage"
    }
  ],
  "scriptFile": "../dist/fileConverter/index.js"
}